<?xml version="1.0" encoding="UTF-8"?><transition_page>
<Title>View Report</Title>
<PageHeading>View Report</PageHeading>
<SummaryContent/>
<ExtraContent>
<h4>Report Parameters</h4>
<div class="para">Enter the parameters and click 'Preview' to view the report, or click on the report type to generate the report.</div>
</ExtraContent>
<Form>
<table class="form">
<tr class="even">
<th>
<span class="required">MyMachine</span>
</th>
<td>Junk</td>
</tr>
<tr class="odd">
<th>
<label for="transition_MyMachine2" class="required">MyMachine2</label>
</th>
<td>
<select id="transition_MyMachine2" name="MyMachine2" class="input" required="required">
<option value="1" selected="selected">Junk</option>
</select>
</td>
</tr>
</table>
<fieldset class="action_buttons">
<button class="input_button" type="submit" title="Generate Preview" name="Preview" value="Preview">
<img class="icon" alt="Preview" src="test/accounting/preview-file-48x48.png"/>Preview</button>
<button class="input_button" type="submit" formtarget="_blank" title="Generate HTML" name="HTML" value="HTML">
<img class="icon" alt="HTML icon" src="test/accounting/html-file-48x48.png"/>HTML</button>
<button class="input_button" type="submit" formtarget="_blank" title="Generate PDF" name="PDF" value="PDF">
<img class="icon" alt="PDF icon" src="test/accounting/pdf-file-48x48.png"/>PDF</button>
<button class="input_button" type="submit" formtarget="_blank" title="Generate CSV" name="CSV" value="CSV">
<img class="icon" alt="CSV icon" src="test/accounting/csv-file-48x48.png"/>CSV</button>
</fieldset>
</Form>
</transition_page>
