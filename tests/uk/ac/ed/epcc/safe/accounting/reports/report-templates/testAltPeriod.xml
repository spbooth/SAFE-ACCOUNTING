<Report 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
     xsi:schemaLocation="http://safe.epcc.ed.ac.uk/report ../../schema/report.xsd"
  xmlns="http://safe.epcc.ed.ac.uk/report"
  xmlns:par="http://safe.epcc.ed.ac.uk/parameter"
  xmlns:per="http://safe.epcc.ed.ac.uk/period"
  xmlns:fil="http://safe.epcc.ed.ac.uk/filter"
  xmlns:tab="http://safe.epcc.ed.ac.uk/table"
  xmlns:cha="http://safe.epcc.ed.ac.uk/chart"
  xmlns:ato="http://safe.epcc.ed.ac.uk/atom">
  
<Title>Test Period</Title>

<Heading>Default Period</Heading>
<cha:TimeChart>
	<cha:NumberOfTimeBlocks>5</cha:NumberOfTimeBlocks>
	<cha:Plot>Wall</cha:Plot>
	
</cha:TimeChart>
<!-- should be the start on the previous month to the start of the current month -->

<Heading>Default End Time</Heading>
<cha:TimeChart>
	<per:Period>
		<per:StartTime>26-09-2008</per:StartTime>
	</per:Period>
	<cha:NumberOfTimeBlocks>5</cha:NumberOfTimeBlocks>
	<cha:Plot>Wall</cha:Plot>
	
</cha:TimeChart>

<Heading>Default Number of Splits</Heading>
<cha:TimeChart>
	<per:Period>
		<per:StartTime>26-09-2008</per:StartTime>
		<per:EndTime>28-09-2008</per:EndTime>
	</per:Period>
	<cha:NumberOfTimeBlocks>5</cha:NumberOfTimeBlocks>
	<cha:Plot>Wall</cha:Plot>
	
</cha:TimeChart>

<Heading>Set Number of Splits</Heading>
<cha:TimeChart>
	<per:Period>
		<per:StartTime>26-09-2008</per:StartTime>
		<per:EndTime>28-09-2008</per:EndTime>
		<per:NumberOfSplits>10</per:NumberOfSplits>
	</per:Period>
	<cha:NumberOfTimeBlocks>5</cha:NumberOfTimeBlocks>
	<cha:Plot>Wall</cha:Plot>
	
</cha:TimeChart>

<Heading>TimeStamp Period</Heading>
<cha:TimeChart>
	<per:Period>
		<per:StartTime>01-10-2008 04:00:00</per:StartTime>
		<per:EndTime>01-10-2008 12:00:00</per:EndTime>
	</per:Period>
	<cha:NumberOfTimeBlocks>5</cha:NumberOfTimeBlocks>
	<cha:Plot>Wall</cha:Plot>
	
</cha:TimeChart>

<Heading>Month Period</Heading>
<cha:TimeChart>
	<per:Period>
		<per:StartTime>09-2008</per:StartTime>
		<per:EndTime>10-2008</per:EndTime>
	</per:Period>
	<cha:NumberOfTimeBlocks>5</cha:NumberOfTimeBlocks>
	<cha:Plot>Wall</cha:Plot>
	
</cha:TimeChart>

<Heading>Year in Quaters Period</Heading>
<cha:TimeChart>
	<per:Period>
		<per:StartTime>09-2008</per:StartTime>
		<per:SplitUnit>Month</per:SplitUnit>
		<per:NumberOfSplitUnits>3</per:NumberOfSplitUnits>		
		<per:NumberOfSplits>4</per:NumberOfSplits>
	</per:Period>
	<cha:NumberOfTimeBlocks>6</cha:NumberOfTimeBlocks>
	<cha:Plot>Wall</cha:Plot>
	
</cha:TimeChart>
	
<Heading>Six One Month Splits</Heading>
<cha:TimeChart>
	<per:Period>
		<per:StartTime>09-2008</per:StartTime>
		<per:SplitUnit>Month</per:SplitUnit>
		<per:NumberOfSplits>6</per:NumberOfSplits>
	</per:Period>
	<cha:NumberOfTimeBlocks>6</cha:NumberOfTimeBlocks>
	<cha:Plot>Wall</cha:Plot>
	
</cha:TimeChart>

<Heading>One Six Month Split</Heading>
<cha:TimeChart>
	<per:Period>
		<per:StartTime>09-2008</per:StartTime>
		<per:SplitUnit>Month</per:SplitUnit>
		<per:NumberOfSplitUnits>6</per:NumberOfSplitUnits>	
	</per:Period>
	<cha:Plot>Wall</cha:Plot>
</cha:TimeChart>

<Heading>One Hour Period</Heading>
<cha:TimeChart>
	<per:Period>
		<per:StartTime>01-10-2008 04:30:00</per:StartTime>
		<per:SplitUnit>Hour</per:SplitUnit>
	</per:Period>
	<cha:NumberOfTimeBlocks>6</cha:NumberOfTimeBlocks>
	<cha:Plot>Wall</cha:Plot>
	
</cha:TimeChart>
<Section>
<Heading>Format Boundary</Heading>
<per:Period>
<per:StartTime>01-10-2008 04:00:00</per:StartTime>
<per:EndTime>01-10-2008 12:00:00</per:EndTime>
</per:Period>
<Text>Start Day <per:StartDate format="dd-MM-yyyy"/></Text>
<Text>Start Time <per:StartDate format="dd-MM-yyyy HH:mm:ss"/></Text>
<Text>Start raw <per:StartDate/></Text>
<Text>End Day <per:EndDate format="dd-MM-yyyy"/></Text>
<Text>End Time <per:EndDate format="dd-MM-yyyy HH:mm:ss"/></Text>
<Text>End raw <per:EndDate/>
</Text>
</Section>
</Report>